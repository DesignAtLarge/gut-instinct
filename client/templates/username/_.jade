                                i.material-icons.prefix(style="margin-top: 10px;") swap_vertical_circle
                                input(id='agidText', class='', placeholder='Please enter your 9-digit American Gut barcode here (this is NOT your Gut Instinct login ID)', value='{{getAGID}}', style="margin-left: 50px;")
