template(name='username')
    nav(style='display: none;')
    |{{init}}
    div(class='container content topics', style='max-width:750px')
        //h2(class='animated fadeIn light-blue-text text-darken-4' style='') Before we begin

        div#ga-username(class='card', style='margin-top: 5%;')
            br
            div(style='margin-left: 10%; margin-right: 10%')
                h5(class='animated fadeIn' style='margin-top: 20px;') Profile
            .card-content(style="margin-right: 30px;")
                form(class='row', style="text-align: left")

                    if preEnterName
                        .input-field.col.s12
                            i.material-icons.prefix(style="margin-top: 10px;") account_circle
                            input(id='userNameAnswerText', class='', placeholder='Enter your username', disabled, value='{{getCurrentName}}', style="margin-left: 50px;")
                    else
                        .input-field.col.s12
                            i.material-icons.prefix(style="margin-top: 10px;") account_circle
                            input(id='userNameAnswerText', class='', placeholder='Enter your username', required, style="margin-left: 50px;")
                    br
                    br
                    if notGalileo
                        h6(class='animated fadeIn' style='margin-left: 9%; margin-top: 20px; text-align: left;') GutInstinct identifies you in your GutInstinct activity using your username. Usernames can only be chosen once, so make it unique.
                    else
                        h6(class='animated fadeIn' style='margin-left: 9%; margin-top: 20px; text-align: left;') Galileo identifies you in your Galileo activity using your username. Usernames can only be chosen once, so make it unique.
                    br
                    br


                    if preEmail
                        .input-field.col.s12
                            i.material-icons.prefix(style="margin-top: 10px;") email
                            input(id='emailAnswerText', class='', placeholder='Provide your email', value='{{getEmail}}', style="margin-left: 50px;")
                    else
                        .input-field.col.s12
                            i.material-icons.prefix(style="margin-top: 10px;") email
                            input(id='emailAnswerText', class='', placeholder='Provide your email (recommended)', style="margin-left: 50px; padding-left: 20px")
                    br
                    br
                    if notGalileo
                        h6(class='animated fadeIn' style='margin-left: 9%; margin-top: 30px; text-align: left;') E-mail updates include feedback from peers and experts! Galileo will only send you e-mails that help you learn more about designing experiments.
                    else
                        h6(class='animated fadeIn' style='margin-left: 9%; margin-top: 30px; text-align: left;') E-mail updates include feedback from peers and experts! Gut Instinct will only send you e-mails that help you learn more about designing experiments.
                    br
                    br

                    if notGalileo
                        h6(class='animated fadeIn' style='margin-left: 9%; margin-top: 30px; text-align: left;') If you have participated in American Gut Project...
                        if preAGID
