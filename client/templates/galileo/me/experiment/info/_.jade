                                                                span
                                                                    a#timeDiffInfo(href="#")
                                                                                i.fa.fa-question-circle
                                                    tr.condition.light-blue-text.text-darken-4
                                                        td(colspan="1000000") Control Condition
                                                tbody
                                                    each participant in getExperimentParticipantsControl
                                                    
                                                        tr
                                                            td
                                                                span.flag-icon(class="{{participant.flag}}")
                                                            td {{participant.participantMap}}
                                                            //- td.hidableCol Yes
                                                            //- td.hidableCol Male
                                                            td.hidableCol {{participant.galileo.city}}
                                                            td.hidableCol.end {{participant.galileo.timezone}}
                                                            
