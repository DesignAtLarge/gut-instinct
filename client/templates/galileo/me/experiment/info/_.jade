template(name="gaMeExperimentInfo")
    +gaShareSocialModal(link=getInviteReviewerLink title=getExpTitleReg)
    +gaShareEmailModal(link=getInviteReviewerLink title=getExpTitleReg)
    +gaShareEmailInviteJoinModal(link=getParticipantInviteLink)
    +gaCorrectnessModal(exp=exp)
    +gaClarificationCreatorModal(title=title expId=expId)
    +gaInformationModal
    
    div#fb-root
    +gaMeExperimentMaster(id=id section=section)
        span(style="display: none;" exp-data-id="{{expId}}" id="exp-id-placeholder") placeholder for data, user should not see this
    
        div#followup-modal.modal
            div.modal-content
                h5#followup-modal-title Title: User shouldn't see this. 
                p#followup-modal-content User shouldn't see this. 
            div.modal-footer
                a.gray-text(class="modal-action modal-close btn-flat") Close

        div#cause-modal.modal
            div.modal-content
                h5 Cause Daily Message sent at {{causeTime}}
                div.container(style="position: relative; text-align: center; width: 368px")
                    img(src='/images/galileo/exp_design/iPhone.png' style="height: 450px")
                    p(style="top: 89px; left: 160px; position: absolute;font-size: 9px; font-weight: 800;color: #d2c8c1;") Today {{causeTime}}
                    p(style="top: 111px; left: 106px; position: absolute; font-size: 10px; font-weight: 400;color: #000000;text-align: left;width: 110px") {{causeMessage}}
            div.modal-footer
                a.gray-text(class="modal-action modal-close btn-flat") Close

        div#effect-modal.modal
            div.modal-content
                h5 Effect Daily Message sent at {{effectTime}}
                div.container(style="position: relative; text-align: center; width: 368px")
                    img(src='/images/galileo/exp_design/iPhone.png' style="height: 450px")
                    p(style="top: 89px; left: 160px; position: absolute;font-size: 9px; font-weight: 800;color: #d2c8c1;") Today {{effectTime}}
                    p(style="top: 111px; left: 106px; position: absolute; font-size: 10px; font-weight: 400;color: #000000;text-align: left;width: 110px") {{effectMessage}}
            div.modal-footer
                a.gray-text(class="modal-action modal-close btn-flat") Close


        if isLoading
            p Loading ...
        else
            section
