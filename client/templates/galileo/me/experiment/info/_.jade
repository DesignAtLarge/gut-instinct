                                            p
                                                a(href="#" id="reviewerUrl" style="font-size: 18px;") {{getInviteReviewerLink}}&nbsp;
                                            p(style="margin-bottom: -10px;")
                                                
                                                //a(id="copyInviteReviewLinkBtn") (Copy)
                                                a.waves-effect.waves-light.btn.light-blue(style="padding-right:15px; padding-left:15px; font-size:9pt; height:27px; top: -10px" id="copyInviteReviewLinkBtn") Copy Link
                                                    i.material-icons.prefix.left content_copy
                                                //- iframe(src='https://www.facebook.com/plugins/share_button.php?href={{getReviewerIframeUrl}}&layout=button&size=large&mobile_iframe=true&width=73&height=28&appId', width='73', height='28', style='border:none;overflow:hidden', scrolling='no', frameborder='0', allowtransparency='true')
                                                img(src="/images/fb_share_icon.png" style="height: 28px; width: auto;" id="facebook-share-icon-btn")
                                                a.twitter-share-button(href="https://twitter.com/share?url={{getInviteReviewerLinkHTML}}&related=twitterapi%2Ctwitter&text=I%20have%20designed%20an%20experiment%20to%20test%20{{getExpTitle}}%2E%0APlease%20help%20me%20improve%20it%20using%20your%20reviews%2E&ref_src=twsrc%5Etfw&size=large") Tweet
                                                script(async='', src='https://platform.twitter.com/widgets.js', charset='utf-8')
                                                //- a.twitter-share-button(href='https://twitter.com/intent/tweet?text=Hello%20world')
                                                //-   | Tweet
                                                a.waves-effect.waves-light.btn.light-blue(style="padding-right:15px; padding-left:15px; font-size:9pt; height:27px; top: -10px" id="openEmailTemplateBtn") Email
                                            input.text(readonly value="{{getInviteReviewerLink}}" id="inviteReviewLink" style="padding-left:10px; height:36px; margin-bottom:-15px; display: none;")
                                            
                                        li
                                            //p: a.btn.blue#inviteFeedback Invite Reviewers
                                            p #[b Use the reviews to improve your experiment design]
                                            //p: a.btn.blue#addCriteriaBtn Decide Participation Criteria
                                            p: a.btn.blue(href="../{{expId}}/design")#viewFeedback View Experiment Reviews
                        else if statusIsReviewOngoing
